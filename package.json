{
  "name": "sushiswap-v1-periphery",
  "version": "1.2.0-RC.0",
  "description": "Peripheral smart contracts for interacting with Sushiswap V1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manifoldfinance/uniswap-v2-periphery"
  },
  "files": [
    "/dist/**/*",
    "/contracts",
    "/contracts/base",
    "/contracts/interfaces",
    "/contracts/libraries",
    "/artifacts/contracts/**/*.json",
    "!artifacts/contracts/**/*.dbg.json",
    "!artifacts/contracts/test/**/*",
    "!artifacts/contracts/base/**/*"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@openzeppelin/contracts": "3.4.2",
    "@uniswap/lib": "4.0.1-alpha",
    "@uniswap/v2-core": "1.0.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typedoc": "^0.22.15",
    "typescript": "4.6.3"
  },
  "scripts": {
    "build": "npx hardhat compile",
    "lint": "yarn prettier ./test/*.ts --check",
    "lint:fix": "yarn prettier ./test/*.ts --write",
    "clean": "rimraf ./build/",
    "copy-v1-artifacts": "ncp ./buildV1 ./build",
    "precompile": "yarn clean",
    "compile": "waffle .waffle.json",
    "postcompile": "yarn copy-v1-artifacts",
    "pretest": "yarn compile",
    "test": "mocha",
    "prepublishOnly": "yarn test"
  },
  "license": "GPL-3.0-or-later"
}
